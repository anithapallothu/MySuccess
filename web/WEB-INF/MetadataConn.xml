<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>pi Metadata Connection Properties</comment>

        <entry key="dbType">oracle</entry>
        <entry key="driverType">thin</entry>
        <entry key="networkProtocol">tcp</entry>
        <entry key="serverPort"></entry>
        <entry key="serverName"></entry>
        <entry key="databaseName">xe</entry>
        <entry key="userName"></entry>
        <entry key="password"></entry>
        <entry key="schemaName"></entry>
        <entry key="maxConnections">50</entry>
        

</properties>
